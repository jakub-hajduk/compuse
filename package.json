{
  "name": "comupse",
  "version": "0.0.0",
  "type": "module",
  "description": "Library for analyzing components usage in various codebases usign any frameworks.",
  "scripts": {
    "dev": "pkgroll --watch",
    "build": "pkgroll",
    "test": "tsx ./scripts/test-runner.ts",
    "check": "biome check",
    "fix": "biome check --fix",
    "release": "pnpm run fix && pnpm run build && changelogen --release --push --publish"
  },
  "keywords": [],
  "author": {
    "name": "Jakub Hajduk",
    "email": "devhajduk@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakub-hajduk/compuse.git"
  },
  "license": "ISC",
  "peerDependencies": {
    "@angular/compiler": "*",
    "@babel/parser": "*",
    "@babel/traverse": "*",
    "@vue/compiler-dom": "*",
    "@vue/compiler-sfc": "*",
    "svelte": "*",
    "vite": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.13.4",
    "changelogen": "^0.5.7",
    "dotenv": "^16.4.7",
    "glob": "^11.0.1",
    "pkgroll": "^2.10.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    }
  },
  "dependencies": {
    "@babel/generator": "^7.27.1",
    "@babel/types": "^7.27.1",
    "get-wild": "^3.0.2"
  }
}
