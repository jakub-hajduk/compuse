{
  "name": "compuse",
  "version": "0.0.1",
  "type": "module",
  "description": "Library for analyzing components usage in various codebases usign any frameworks.",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "tsx ./scripts/test-runner.ts",
    "test:watch": "tsx ./scripts/test-runner.ts watch",
    "check": "biome check",
    "fix": "biome check --fix",
    "release": "pnpm run fix && pnpm run build && changelogen --release --push --publish"
  },
  "keywords": [
    "components",
    "usage",
    "angular",
    "vue",
    "react",
    "svelte",
    "lit-html",
    "web components",
    "zero dependency",
    "template"
  ],
  "author": {
    "name": "Jakub Hajduk",
    "email": "devhajduk@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakub-hajduk/compuse.git"
  },
  "license": "ISC",
  "dependencies": {
    "fragmint": "^0.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.13.4",
    "changelogen": "^0.5.7",
    "dotenv": "^16.4.7",
    "glob": "^11.0.1",
    "tsdown": "^0.16.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "files": ["dist"],
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./analyze": {
      "import": "./dist/analyze.mjs",
      "require": "./dist/analyze.cjs"
    },
    "./angular": {
      "import": "./dist/angular.mjs",
      "require": "./dist/angular.cjs"
    },
    "./html": {
      "import": "./dist/html.mjs",
      "require": "./dist/html.cjs"
    },
    "./lit": {
      "import": "./dist/lit.mjs",
      "require": "./dist/lit.cjs"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.cjs"
    },
    "./svelte": {
      "import": "./dist/svelte.mjs",
      "require": "./dist/svelte.cjs"
    },
    "./vue": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.cjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.cts"
}
